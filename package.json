{
  "name": "kevinpatto-dot-com",
  "version": "1.0.0",
  "description": "kevin patto's website",
  "main": "server/server.js",
  "scripts": {
    "start": "node server --ignore client",
    "develop": "concurrently \"cd server && npm run start\" \"cd client && npm run dev\"",
